<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neuedu.dao.IOrderDao">

    <select id="findOrderItemsByOrderNo" parameterType="long" resultType="orderItem">
        select id,user_id,order_no,product_id,product_id,product_name,product_image,current_unit_price,quantity,total_price,
                create_time, update_time
        from neuedu_order_item
        where order_no=#{orderno}
    </select>

</mapper>
